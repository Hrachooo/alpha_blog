<h1>Listing the table of articles</h1>
<table>
    <thead>
        <tr>
            <th>Title </th>
            <th>Descritpion </th>
            <th>Folder </th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |article|%>
            <tr>
                <td><%= article.title %></td>
                <td><%= article.description%></td>
                <td>Blank</td>
            </tr>
        <% end %> 
    </tbody>
</table>
